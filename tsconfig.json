{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/_app.js", "pages/api/db/getRides.ts", "context/UberContext.js", "pages/index.js", "components/Confirm.tsx", "components/LocationSelector.tsx", "components/RiderSelector.tsx", "components/Map/Map.jsx", "components/Navbar/RightMenu.tsx", "components/Navbar/NavBar.tsx", "components/Navbar/LeftMenu.tsx"],
  "exclude": ["node_modules"]
}
